{
  "name": "bedrock-protractor",
  "version": "3.3.1-0",
  "description": "Bedrock Protractor-based Test Framework",
  "main": "lib",
  "browser": false,
  "scripts": {
    "postinstall": "webdriver-manager update",
    "postupdate": "webdriver-manager update"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/digitalbazaar/bedrock-protractor"
  },
  "keywords": [
    "bedrock"
  ],
  "author": {
    "name": "Digital Bazaar, Inc.",
    "email": "support@digitalbazaar.com",
    "url": "http://digitalbazaar.com"
  },
  "bugs": {
    "url": "https://github.com/digitalbazaar/bedrock-protractor/issues"
  },
  "homepage": "https://github.com/digitalbazaar/bedrock-protractor",
  "dependencies": {
    "chai": "^4.1.0",
    "chai-as-promised": "^7.1.1",
    "lodash": "^4.3.0",
    "mocha": "^3.4.2",
    "protractor": "^5.1.2",
    "temp": "~0.8.1"
  },
  "peerDependencies": {
    "bedrock": "^1.6.0",
    "bedrock-express": "^2.0.0",
    "bedrock-server": "^2.0.0",
    "bedrock-views": "digitalbazaar/bedrock-views#overrideDeps"
  },
  "bedrock": {
    "browserDependencies": [
      "chai",
      "chai-as-promised",
      "mocha"
    ],
    "manifest": {
      "chai": {
        "browser": "chai.js"
      },
      "chai-as-promised": {
        "browserDependencies": [
          "check-error"
        ]
      }
    },
    "systemjs": {
      "packages": {
        "mocha": {
          "format": "global",
          "defaultExtension": "js",
          "main": "mocha.js"
        }
      }
    }
  },
  "directories": {
    "lib": "./lib"
  }
}
